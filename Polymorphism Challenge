class Vehicle:
    def move(self):
        raise NotImplementedError("Subclasses must implement this method")

class Car(Vehicle):
    def move(self):
        print("Driving ğŸš—")

class Bicycle(Vehicle):
    def move(self):
        print("Pedaling ğŸš²")

class Airplane(Vehicle):
    def move(self):
        print("Flying âœˆï¸")

vehicles = [Car(), Bicycle(), Airplane()]

for vehicle in vehicles:
    vehicle.move()
